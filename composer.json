{
    "name": "khmer-pdf/laravel-kh-pdf",
    "type": "package",
    "description": "The simple package laravel for support khmer font with pdf with mPdf",
    "keywords": ["pdf", "package", "mPdf", "laravel","laravel pdf khmer", "khmer", "khmer pdf"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KhmerPdf\\LaravelKhPdf\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "mpdf/mpdf": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KhmerPdf\\LaravelKhPdf\\Providers\\PdfKhServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "nuon",
            "email": "duchnuon@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
